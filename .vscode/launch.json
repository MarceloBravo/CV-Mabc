    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Nodemon Debug",
            "runtimeExecutable": "nodemon",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\backend-cv\\index.js",
            "restart": true,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "env":{
                "HOST": "localhost",
                "HOST_TEST": "localhost",
                "PORT": "5433",
                "PORT_TEST": "5433",
                "DB": "mabc_cv",
                "DB_TEST": "mabc_cv_test",
                "PWD": "123456",
                "APP_PORT": "3000",

                "DEFAULT_REG_POR_PAGINA": "10",
            }
        }
    ]
}